{% extends "dashboard.html.twig" %}

{% block body %}
    <h1>Mis estudiantes</h1>
     <script>
        localStorage.setItem("menuLateral", "acudientes-estudiantes");
    </script>
<div class="col-lg-4">
<table class="table table-bordered">
	<thead>
		<tr>
			<th>No.</th>
			<th>Nombres</th>
		</tr>
	</thead>
	<tbody>
		{% for estudiante in estudiantes %}
		<tr>
			<td>{{ loop.index }}</td>
			<td>{{ estudiante.estudiante }}</td>
		</tr>
		{% endfor %}
	</tbody>
</table>
</div>    
{% endblock %}