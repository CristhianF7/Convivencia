{% extends "dashboard.html.twig" %}

{% block body %}
<div class="col-lg-6">
    <h1>TipoFalta</h1>

    <table class="table table-bordered">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ tipoFaltum.id }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ tipoFaltum.descripcion }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('tipofalta_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('tipofalta_edit', { 'id': tipoFaltum.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
</div>
{% endblock %}
